<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootPane" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Controllers.SettingsController">
   <children>
      <Label text="Nastavenia">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="addDocumentButton" mnemonicParsing="false" text="Pridať dokument" />
      <Button fx:id="changePasswordButton" mnemonicParsing="false" text="Zmeniť heslo" />
      <Button fx:id="editCustomerButton" mnemonicParsing="false" text="Upraviť zákazníka" />
      <Button fx:id="backButton" mnemonicParsing="false" text="Späť" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
